            app:layout_constraintBottom_toTopOf="@+id/button6" tools:text="Agregar"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button6" android:layout_marginEnd="163dp" android:layout_marginRight="163dp"
